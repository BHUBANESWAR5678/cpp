#include <iostream>

int main() {
    int num1, num2, remainder;

    std::cout << "Enter dividend: ";
    std::cin >> num1;

    std::cout << "Enter divisor: ";
    std::cin >> num2;

    if (num2 != 0) {
        remainder = num1 % num2;
        std::cout << "Remainder: " << remainder << std::endl;
    } else {
        std::cout << "Cannot calculate remainder with zero divisor." << std::endl;
    }

    return 0;
}
