#include <iostream>

int main() {
    int num1, num2;
    float quotient;

    std::cout << "Enter dividend: ";
    std::cin >> num1;

    std::cout << "Enter divisor: ";
    std::cin >> num2;

    if (num2 != 0) {
        quotient = static_cast<float>(num1) / num2;
        std::cout << "Quotient: " << quotient << std::endl;
    } else {
        std::cout << "Cannot divide by zero." << std::endl;
    }

    return 0;
}
