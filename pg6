#include <iostream>

int main() {
    float num1, num2, num3, average;

    std::cout << "Enter first number: ";
    std::cin >> num1;

    std::cout << "Enter second number: ";
    std::cin >> num2;

    std::cout << "Enter third number: ";
    std::cin >> num3;

    average = (num1 + num2 + num3) / 3;

    std::cout << "Average: " << average << std::endl;

    return 0;
}
